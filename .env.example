# Application
NEXT_PUBLIC_APP_NAME=AIR
NODE_ENV=development

# Database
DATABASE_URL="postgresql://air_user:air_password@localhost:5432/air_db?schema=public"

# Redis
REDIS_URL="redis://localhost:6379"
